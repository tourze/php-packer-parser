{
  "name": "tourze/php-packer-parser",
  "description": "PHP代码解析器，用于处理PHP代码及其依赖的解析和处理",
  "type": "library",
  "require": {
    "php": "^8.1",
    "tourze/php-packer-ast": "0.0.*",
    "tourze/php-packer-analysis": "0.0.*",
    "psr/log": "^3|^2|^1",
    "symfony/stopwatch": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PhpPacker\\Parser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpPacker\\Parser\\Tests\\": "tests/"
    }
  }
}